    "@stripe/stripe-js": "^2.1.11",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
