{
  "name": "voluns",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run validate-css && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:css": "jest --testPathPattern=css",
    "validate-css": "node scripts/validate-css.js",
    "check-dark-mode": "node scripts/check-dark-mode.js",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "validate": "npm run type-check && npm run lint && npm run validate-css && npm run test",
    "clean": "rm -rf .next node_modules",
    "prebuild": "npm run validate-css"
  },
  "dependencies": {
